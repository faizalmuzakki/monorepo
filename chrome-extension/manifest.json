{
  "manifest_version": 3,
  "name": "Website Blocker - Workspace Manager",
  "version": "1.0.0",
  "description": "Block distracting websites with workspace-specific settings for different browser windows. Stay focused and productive!",
  "author": "Your Name",
  "homepage_url": "https://github.com/faizalmuzakki/monorepo",
  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Website Blocker"
  },
  "web_accessible_resources": [
    {
      "resources": ["blocked.html"],
      "matches": ["<all_urls>"]
    }
  ]
}
