{
  "manifest_version": 3,
  "name": "Website Blocker",
  "version": "1.0.0",
  "description": "Block access to specific websites you want to avoid",
  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    {
      "resources": ["blocked.html"],
      "matches": ["<all_urls>"]
    }
  ]
}
